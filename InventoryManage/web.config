<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <aspNetCore forwardWindowsAuthToken="false" processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%"
                stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout">
      <environmentVariables>
        <!-- environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Development" /-->
      </environmentVariables>
    </aspNetCore>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
    </handlers>
  </system.webServer>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Server=(localdb)\.\aaaIISDB;Database=InventoryManage;Trusted_Connection=True;MultipleActiveResultSets=true"/>
  </connectionStrings>
</configuration>